{
  "name": "revcat-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "build:web": "expo export --platform web",
    "build:ios": "eas build --platform ios",
    "build:android": "eas build --platform android",
    "build:all": "eas build --platform all",
    "e2e": "maestro test .maestro",
    "e2e:auth": "maestro test .maestro/auth",
    "e2e:payments": "maestro test .maestro/payments",
    "update": "eas update --channel production",
    "update:preview": "eas update --channel preview",
    "generate": "node scripts/generate.js",
    "prepare": "husky"
  },
  "dependencies": {
